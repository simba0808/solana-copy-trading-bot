{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@commands/*": [
        "commands/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@actions/*": [
        "actions/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@store/*": [
        "store/*"
      ]
    }
  },
  "include": [
    "config/**/*",
    "commands/**/*",
    "models/**/*",
    "actions/**/*",
    "utils/**/*",
    "store/**/*",
    "index.js"
  ]
}